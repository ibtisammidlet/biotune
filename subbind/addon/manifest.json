      "run_at": "document_idle"
   }, {
      "js": [ "./lib/bindingdb_rwd.js" ],
      "matches": [ "*://*.bindingdb.org/rwd/*" ],
      "run_at": "document_idle"
   }, {
      "js": [ "./lib/dezgo.js" ],
      "matches": [ "*://*.dezgo.com/?query=*" ],
      "run_at": "document_idle"
